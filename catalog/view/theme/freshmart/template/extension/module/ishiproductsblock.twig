<section id="{{ ishi_randomnumer }}" class="ishiproductsblock">
  <h3 class="home-title">{{ heading }}</h3>
  <ul class="ishiproductstab nav nav-tabs clearfix">
    {% if featured %}
      <li class="nav-item active">
        <a class="nav-link" href="#featured-products-block{{ tab_randomnumer }}" data-toggle="tab">{{ heading_featured }}</a>
      </li>
    {% endif %}
    {% if bestseller %}
      <li class="nav-item">
        <a class="nav-link" href="#bestseller-products-block{{ tab_randomnumer }}" data-toggle="tab">{{ heading_bestseller }}</a>
      </li>
    {% endif %}
    {% if new %}
      <li class="nav-item">
        <a class="nav-link" href="#new-products-block{{ tab_randomnumer }}" data-toggle="tab">{{ heading_new }}</a>
      </li>
    {% endif %}
    {% if special %}
      <li class="nav-item">
        <a class="nav-link" href="#special-products-block{{ tab_randomnumer }}" data-toggle="tab">{{ heading_special }}</a>
      </li>
    {% endif %}
	{% if category %}
		{% for category in product_categories %}
		  <li class="nav-item">
			<a class="nav-link" href="#category-block{{ category.category_id }}-{{ tab_randomnumer }}" data-toggle="tab">{{ category.name }}</a>
		  </li>
		  {% endfor %}
		{% endif %}
	</ul>

  <div class="tab-content">
    <div class="tab-pane active" id="featured-products-block{{ tab_randomnumer }}">
      <div class="block_content row">        
        <div class="owl-carousel"> 
            {% set counter = 1 %}
            {% set lastproduct = featured|length %} 
            {% for product in featured %}
              {% if product_row != 1 %}
                {% if counter % product_row == 1 %}
                  <div class="multilevel-item">
                {% endif %}
              {% endif %}
              <div class="product-container">
                  <div class="item product-thumb" data-countdowntime="{{ product.date_end }}"> 
                    <div class="image">
                      <a href="{{ product.href }}">
                        <img src="{{ product.thumb }}" alt="{{ product.name }}" title="{{ product.name }}" class="img-responsive"/>
                        {% if product.extra != '' and hover_image == 1 %}
                          <img class="product-img-extra img-responsive" alt="{{ product.name }}" title="{{ product.name }}" src= "{{ product.extra }}"/>
                        {% endif %}
                      </a>  
                      {% if product.stock_status %}<span class="outstock-overlay">{{ product.stock_status }}</span>{% endif %}
                      {% if product.rating %}
                        <div class="rating">
                          {% for i in 1..5 %}
                            {% if product.rating < i %} 
                              <span class="fa fa-stack">
                                <i class="fa fa-star gray fa-stack-2x"></i>
                              </span> 
                            {% else %} 
                              <span class="fa fa-stack">
                                <i class="fa fa-star yellow fa-stack-2x"></i>
                              </span> 
                            {% endif %}
                          {% endfor %}
                        </div>
                        {% else %}
                        <div class="rating">
                          {% for i in 1..5 %}
                          <span class="fa fa-stack">
                            <i class="fa fa-star gray fa-stack-2x"></i>
                            </span>
                          {% endfor %}
                        </div>
                      {% endif %} 
                      {% if product_counter == 1 %}
                        {% if product.date_end %}
                          <div class='countdown-container'>
                            <div class='countdown-days counter'>
                              <span class="data"></span>
                              <span class="lbl">{{ days_name }}</span>
                            </div>
                            <div class='countdown-hours counter'>
                              <span class="data"></span>
                              <span class="lbl">{{ hours_name }}</span>
                            </div>
                            <div class='countdown-minutes counter'>
                              <span class="data"></span>
                              <span class="lbl">{{ min_name }}</span>
                            </div>
                            <div class='countdown-seconds counter'>
                              <span class="data"></span>
                              <span class="lbl">{{ sec_name }}</span>
                            </div>
                          </div>
                        {% endif %} 
